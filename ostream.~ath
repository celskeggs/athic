// import method strlen;
import native write;

strlen = ~ATH(THIS) {
	core EXECUTE THIS;
	length = core.THIS - THIS;
};
strlen.core = ~ATH(@THIS) {
	THIS = THIS + 1;
};

EXPORT = ~ATH(NULL) {
	write = NULL;
};
EXPORT.write = ~ATH(THIS) {
	strlen EXECUTE THIS;
//	write.count = strlen.length;
//	write.fd = 1;
	write EXECUTE THIS;
	THIS = NULL;
};
EXPORT.write.strlen = strlen;
