// import method strlen;
import native write;

write.eax = 0x04;

strlen = ~ATH(THIS) {
	core EXECUTE THIS;
	length = core.THIS - THIS;
};
strlen.core = ~ATH(@THIS) {
	THIS = THIS + 1;
};

EXPORT = ~ATH(NULL) {
	write = NULL;
};
EXPORT.write = ~ATH(THIS) {
	strlen EXECUTE THIS;
	write.ebx = 1;
	write.ecx = THIS;
	write.edx = strlen.length;
	write EXECUTE NULL;
	THIS = NULL;
};
EXPORT.write.strlen = strlen;
